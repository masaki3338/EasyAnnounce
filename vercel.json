{
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/manual", "destination": "/manual.pdf" },

    { "source": "/api/tts-voicevox/(.*)", "destination": "https://voicevox-engine-l6ll.onrender.com/$1" }
  ],
  "functions": {
    "api/tts-voicevox.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "/api/tts-voicevox(?:/.*)?", "dest": "/api/tts-voicevox.js" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
