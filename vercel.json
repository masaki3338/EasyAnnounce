{
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "routes": [
    { "src": "/field.jpg", "dest": "/field.jpg" },
    { "src": "/manual.pdf", "dest": "/manual.pdf" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
